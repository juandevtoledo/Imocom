public class UtilException extends GenericException {    
    
    /**
     *
     * @param exception
     * @param level
     */
    public UtilException(Exception exception, Level level) {
        super(exception, level);
    }   
    
    /**
     *
     * @param message
     * @param level
     */
    public UtilException(String message, Level level) {
        super(message, level);
    }     
}

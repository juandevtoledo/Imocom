HTML {
    padding: 0;
    margin: 0 auto;
    font-family:Verdana;
    font-size: 14px;
    background: rgb(209,216,199); /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, rgba(209,216,199,1) 1%, rgba(252,255,244,1) 53%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,rgba(209,216,199,1)), color-stop(53%,rgba(252,255,244,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, rgba(209,216,199,1) 1%,rgba(252,255,244,1) 53%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, rgba(209,216,199,1) 1%,rgba(252,255,244,1) 53%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, rgba(209,216,199,1) 1%,rgba(252,255,244,1) 53%); /* IE10+ */
    background: linear-gradient(to bottom, rgba(209,216,199,1) 1%,rgba(252,255,244,1) 53%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d1d8c7', endColorstr='#fcfff4',GradientType=0 ); /* IE6-8 */

    background-repeat: no-repeat;

}


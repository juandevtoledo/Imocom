/*
 * Table styles estilos propios
 */

/* estilos para tablas de resultados*/

.tablaresultados table {
    width: 100%;
    border-collapse: collapse;
    border: 1px solid #38678f;
    margin: 50px auto;
    background: white;
}

/*Estilo para las cabeceras de las tablas de resultados*/
.tablaresultados thead th {
    background: none repeat scroll 0% 0% #4682B4;
    height: 54px;
    font-weight: bold;
    text-shadow: 0px 1px 0px #38678F;
    color: #FFF;
    border: 1px solid #38678F;
    box-shadow: 0px 1px 2px #568EBD inset;
    transition: all 0.2s ease 0s;
    font-size: 1.4em;
}

.tablaresultados tbody {
    font: 400 16px 'Muli', sans-serif !important;
    margin: 0;
    padding: 0;
    text-align: center;  
}
.tablaresultados tbody tr td{
    text-align: center;
    border: 1px solid #38678f;
}
.requerido {
    color: red; 
    font-weight: bold !important;
    display: inline-block !important;
}

.tablaresultados .backGIntelcom{
    text-align: center;
    background-color: #009ACD;
    color: #FFF;
    margin: 0 auto;
}
/*FIN estilos tabla resultados*/

.labelForm {
    display: inline-block !important;
    font-weight: bold !important;


}

.messagesForm {
    color: red !important; 
    font-weight: bold !important;

}

/* estilos para tablas de detalles*/
.tablaDetalles {
    width: 90%;
    border: 1px solid steelblue;

}

.tablaDetalles tbody tr td{
    text-align: center;
    border: 1px solid steelblue;
    padding: 0;
}

.labelDetalle{
    background: steelblue !important;
    background-color: steelblue !important;
    text-align: center !important;
    color: white !important;
    margin: 0 !important;
    padding: 4px 0;
    font-weight: bold !important;
}

/*FIN de estilos para tablas de detalles */

